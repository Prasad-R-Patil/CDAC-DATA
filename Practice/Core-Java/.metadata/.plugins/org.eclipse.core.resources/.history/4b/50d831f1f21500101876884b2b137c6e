package campus.accounts;

import java.util.Date;

import campus.academics.Student;

public class Fees extends Student {
	
	protected int amount;
	
	protected Date payDate;

	public Fees() {
		super();
		
	}
	
	public Fees(int amt, Date dt) {
		super();
		
		this.amount=amt;
		this.payDate=dt
		
	}

	
	
	
	

}
